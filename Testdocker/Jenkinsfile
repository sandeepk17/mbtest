pipeline {
    environment {
         vari = ""
    }

    agent any

    stages{
        stage('Build'){
            steps {
                script{
                    echo 'Hello'
                    echo "${params.Env}"
                    echo "${params.Test}"
                    echo "${params.Server}"
                    echo "${params.Multi}"
                    if (params.Server.equals("Could not get Environment from Env Param")) {
                      echo "Must be the first build after Pipeline deployment.  Aborting the build"
                      currentBuild.result = 'ABORTED'
                      return
                    }
                    echo "Crossed param validation"
                } 
            }
        }
        stage ("Test") {
            steps {
                script{
                    echo 'Hello'
                }
            }
        }
    }
}